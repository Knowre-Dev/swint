{
	"rules": {
		"indent": [
			2,
			"tab",
			{
				"SwitchCase": 1,
				"VariableDeclarator": 1
			}
		],
		"quotes": [
			2,
			"single"
		],
		"linebreak-style": [
			2,
			"unix"
		],
		"semi": [
			2,
			"always"
		]
	},
	"globals": {
		"print": true
	},
	"env": {
		"es6": true,
		"node": true
	},
	"extends": "eslint:recommended"
}
